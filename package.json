{
  "name": "nestjs-wemo",
  "version": "0.0.1",
  "description": "WEMO project",
  "author": "Ben Hu",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typeorm": "env-cmd ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:generate",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:run": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:run",
    "migration:revert": "npm run typeorm -- --dataSource=src/database/data-source.ts migration:revert",
    "schema:drop": "npm run typeorm -- --dataSource=src/database/data-source.ts schema:drop",
    "seed:run": "ts-node -r tsconfig-paths/register ./src/database/seeds/run-seed.ts",
    "prebuild": "rimraf dist",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:coverage": "npm run test -- --coverage --verbose",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "env-cmd jest --config ./test/jest-e2e.json --runInBand",
    "test:e2e:docker": "sudo docker-compose -f docker-compose.ci.yaml --env-file env-example -p ci up --build --exit-code-from api && docker-compose -p ci rm -svf",
    "clean:git:remote": "git fetch --prune",
    "clean:git:local": "git-removed-branches --prune",
    "clean:git": "run-s clean:git:*",
    "clean:build": "del-cli build -f",
    "clean:coverage": "del-cli coverage -f",
    "clean:dist": "del-cli dist -f",
    "clean:tmp": "del-cli tmp -f",
    "clean": "run-p clean:build clean:coverage clean:dist",
    "fix": "run-s fix:*",
    "lint:ts": "eslint -c .eslintrc.js ./{src,test}/**/*.ts --no-error-on-unmatched-pattern",
    "lint:format": "prettier --config ./prettierrc.json --list-different \"./{{src,test}/**/*,*}.{js,ts,json,md,css,scss,html}\"",
    "fix:format": "npm run lint:format -- --write",
    "fix:ts": "npm run lint:ts -- --fix",
    "build": "run-s build:*",
    "build:nest": "nest build",
    "pre:dev": "run-s clean fix build test:coverage"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/mapped-types": "^1.1.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^6.0.4",
    "@nestjs/typeorm": "^9.0.0",
    "@types/supertest": "2.0.12",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "moment": "^2.29.4",
    "mysql2": "^2.3.3",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "supertest": "6.2.4",
    "typeorm": "0.3.10",
    "typeorm-seeding": "^1.6.1",
    "webpack": "^5.74.0"
  },
  "devDependencies": {
    "@golevelup/nestjs-testing": "^0.1.2",
    "@golevelup/ts-jest": "^0.3.3",
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "27.5.0",
    "@types/node": "^16.0.0",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "better-sqlite3": "^7.4.3",
    "del-cli": "^5.0.0",
    "env-cmd": "10.1.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.1",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.0.0",
    "ttypescript": "^1.5.13",
    "typescript": "^4.3.5"
  }
}